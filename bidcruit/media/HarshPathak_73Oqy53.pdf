    <script>
        $("div[id^='HireModal']").each(function(){
             var HireModal = $(this);
             HireModal.find('.preference-submit').click(function(){
                  var preferenceForm = document.getElementById("preferenceForm");
                  var formData = new FormData(preferenceForm);
                  console.log('formData preferenceForm >>>>>>>',formData)
                  $.ajax({
                          url:"/company/candidate_preference_check",
                          headers: {'X-CSRFToken': $("input[name=csrfmiddlewaretoken]").val() },
                          type:'POST',
                          contentType: false,
                          processData: false,
                          data: formData,
                          error: function (request, status, error) {
                              alert(error);
                          }
                  }).done(function(response){
                      if(response=="True"){
                          HireModal.modal('hide');
                          HireModal.closest("div[id^='HireModal']").nextAll("div[id^='HireModal']").first().modal('show');
                      }else{
                          console.log("false");
                      }
                  })
             });
             HireModal.find('.hire-message-submit').click(function(){
                  var HireMessageForm = document.getElementById("HireMeForm");
                  var formData = new FormData(HireMessageForm);
                  console.log('formData HireMessageForm >>>>>>>',formData)
                  $.ajax({
                          url:"/company/candidate_hire",
                          headers: {'X-CSRFToken': $("input[name=csrfmiddlewaretoken]").val() },
                          type:'POST',
                          contentType: false,
                          processData: false,
                          data: formData,
                          error: function (request, status, error) {
                              alert(error);
                          }
                  }).done(function(response){
                      if(response=="True"){
                           window.location.reload()
                      }else{
                          console.log("false");
